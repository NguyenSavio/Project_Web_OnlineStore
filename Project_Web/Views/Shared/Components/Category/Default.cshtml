@*@model Project_Web.Data.Category*@
@model List<Project_Web.Data.Category>
<form>
    @for (int cate = 0; cate < Model.Count; cate++)
    {
        @if (cate % 2 == 0)
        {
            <ul id="list">

                <li class="color"><a href="#">@Model[cate].Name</a></li>
            </ul>
        }
        else
        {
            <ul id="list">
                <li ><a href="#">@Model[cate].Name</a></li>
            </ul>
        }


    }
</form>



